{
  "server": {
    "host": "0.0.0.0",
    "port": 9000
  },
  "stream": {
    "chunkSize": 2
  },
  "token": {
    "refreshRetryMax": 3,
    "refreshRetryIntervalMs": 60000,
    "refreshBufferMinutes": 5
  },
  "connectionPool": {
    "maxSockets": 20,
    "maxFreeSockets": 10,
    "socketTimeout": 60000,
    "requestTimeout": 30000
  },
  "account": {
    "multiAccountEnabled": true,
    "strategy": "auto",
    "autoSwitchOnError": true
  },
  "logging": {
    "level": "INFO",
    "rotation": {
      "maxSize": 10485760,
      "maxFiles": 5
    }
  }
}